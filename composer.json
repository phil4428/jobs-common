{
    "name": "phil4428/jobs-common",
    "type": "library",
    "description": "Making it simple to integrate your application with job api providers.",
    "keywords": [
        "job boards",
        "jobs",
        "api client",
        "object"
    ],
    "homepage": "http://jobapis.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Karl Hughes",
            "email": "khughes.me@gmail.com",
            "homepage": "http://www.karllhughes.com"
        },
        {
            "name": "Steven Maguire",
            "email": "stevenmaguire@gmail.com",
            "homepage": "https://github.com/stevenmaguire"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "~7.5"
    },
    "require-dev": {
        "phpunit/phpunit":"~10.0-dev",
        "phpunit/php-code-coverage": "~10.0",
        "mockery/mockery": "~1.5",
        "squizlabs/php_codesniffer": "~4.0-dev"
    },
    "autoload": {
        "psr-4": {
            "JobApis\\Jobs\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JobApis\\Jobs\\Client\\Tests\\": "tests/src/",
            "JobApis\\Jobs\\Client\\Fixtures\\": "tests/fixtures/"
        }
    }
}
